#!/usr/bin/env python3

import speech_recognition as sr
import socket


class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.executing = True
        self.logger.info("==========>Init Function worked")

    def onLoad(self):
        #put initialization code here
        self.logger.info("==========>onLoad Function worked")
        pass

    def onUnload(self):
        self.executing = False
        self.outputString("What")
        #self.outputError("Sorry, I spaced out. Can you say that again?")
        self.logger.info("==========>unLoad function worked")
        pass

    def onInput_wavPath(self, p):

        self.executing = True

        self.logger.info("==========>onInput_wavPath Function")

        WAV_FILE = p #get wav file path

        self.logger.info("==========>path to the raw file Got it")


        r = sr.Recognizer()
        with sr.WavFile(WAV_FILE) as source:
            audio = r.record(source) # read the entire WAV
            self.logger.info("==========>Reading the entire file")

        try:
            print "---------------- Detecting -------------------"
            response = r.recognize_google(audio)
            self.logger.info("==========>Words translated")
            print "--------------------------------------------------Google Speech Recognition thinks you said " + response
            if self.executing:
                print "==========>pushing to chat bot"
                self.logger.info("==========>pushing to chat bot")
                self.outputString(str(response))
                self.logger.info("==========>pushing to chat bot done")

        except sr.UnknownValueError:
            print " ----------------------------------------------------------Google Speech Recognition could not understand audio"
            self.outputString(str(response))
            self.logger.info("==========>output error happend")
            #self.outputError("Can you repeat that? I couldn't understand you.")  #output error
        except sr.RequestError as e:
            #print "Could not request results from Google Speech Recognition service; {0}".format(e)
            self.outputString(str(response))
            self.logger.info("==========>request error happend")
            #self.outputError("I can't reach google! The internet must be broken!") #output another error
        except sr.URLError as e:
            #self.outputError("Something went wrong. Idk what it is but though.")
            self.outputString(str(response))
            self.logger.info("==========>URL Error happened")
        except socket.timeout:
            #self.outputError("One of my sockets timed out!")
            self.outputString(str(response))
            self.logger.info("==========>Time out Error Happened")
            print "Timed out!"
        pass

    def onInput_stop(self):
        self.onUnload()
        self.logger.info("==========>get words block stopped")]]>
</content>
